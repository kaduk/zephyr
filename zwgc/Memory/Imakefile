OBJS = new_memory.o
SRCS = new_memory.c

all:: memory_spec $(OBJS)

/**/# Automatically generate a spec file from the template include file:
memory_spec: new_memory.h
	cat new_memory.h | grep "^.\*" | grep -v "^ \*/" | sed 's/.\*//' > memory_spec

clean::
	$(RM) $(OBJS) memory_spec

